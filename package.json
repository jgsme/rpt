{
  "private": true,
  "name": "rpt",
  "description": "random photo from tumblr",
  "scripts": {
    "build": "mkdir -p build && npm run build:css && npm run build:js",
    "build:css": "stylus -c -o build/index.css src/index.styl",
    "build:js": "lsc -cbo build src/index.ls",
    "clean": "rm -rf build",
    "prestart": "npm run build",
    "start": "babel-node index.js"
  },
  "author": "jigsaw (http://jgs.me)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "kcors": "^1.3.1",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-jade": "^2.1.0",
    "koa-router": "^7.1.0",
    "livescript": "^1.5.0",
    "node-base64-image": "^1.0.3",
    "request": "^2.79.0",
    "stylus": "^0.54.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  }
}
